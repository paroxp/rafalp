{
  "name": "rafalp",
  "version": "4.0.0",
  "description": "Rafal Proszowski - Resume",
  "main": "src/generator.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paroxp/rafalp.git"
  },
  "author": "Rafal Proszowski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paroxp/rafalp/issues"
  },
  "homepage": "https://github.com/paroxp/rafalp#readme",
  "scripts": {
    "build:html": "ts-node src/generator.ts",
    "build:javascript": "tsc src/js/*.ts --outDir build/js",
    "build:scss": "node-sass --output-style compressed --include-path src/scss --include-path node_modules/susy/sass src/scss/app.scss --output build/css",
    "build": "npm run -s build:scss && npm run -s build:javascript && npm run -s build:html",
    "fix:ts": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "fix:vulnerabilities": "npm audit fix --update-binary",
    "fix": "npm run -s fix:sass && npm run -s fix:ts",
    "lint:sass": "",
    "lint:ts": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint": "npm run -s lint:sass && npm run -s lint:ts",
    "start": "http-server ./dist",
    "test:audit": "npm audit",
    "test": "npm run -s lint"
  },
  "devDependencies": {
    "@types/html-minifier": "^4.0.2",
    "@types/moment": "^2.13.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-plugin-functional": "^6.0.0",
    "eslint-plugin-import": "^2.28.1",
    "html-minifier": "^4.0.0",
    "http-server": "^14.1.1",
    "moment": "^2.29.4",
    "node-sass": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sitemap": "^7.1.1",
    "susy": "^3.0.8",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  }
}

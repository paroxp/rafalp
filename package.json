{
  "name": "rafalp",
  "version": "3.0.0",
  "description": "Rafal Proszowski - Resume",
  "main": "src/app.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paroxp/rafalp.git"
  },
  "author": "Rafal Proszowski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paroxp/rafalp/issues"
  },
  "homepage": "https://github.com/paroxp/rafalp#readme",
  "scripts": {
    "build": "npm run -s build:scss",
    "build:scss": "node-sass --output-style compressed --include-path src/scss --include-path node_modules/susy/sass src/scss/app.scss --output dist/css",
    "fix": "npm run -s fix:sass && npm run -s fix:ts",
    "fix:sass": "",
    "fix:ts": "tslint -p . --fix",
    "fix:vulnerabilities": "npm audit fix --update-binary",
    "lint": "npm run -s lint:sass && npm run -s lint:ts",
    "lint:sass": "",
    "lint:ts": "tslint --format codeFrame -p .",
    "start": "ts-node ./src/app.ts",
    "test": "npm run -s lint",
    "test:audit": "npm audit"
  },
  "devDependencies": {
    "@koa/router": "^8.0.2",
    "@types/koa": "^2.0.52",
    "@types/koa-static": "^4.0.1",
    "@types/koa__router": "^8.0.2",
    "@types/moment": "^2.13.0",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "koa": "^2.11.0",
    "koa-static": "^5.0.0",
    "moment": "^2.24.0",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "eslint": "^7.8.1",
    "eslint-plugin-functional": "^3.0.2",
    "eslint-plugin-import": "^2.22.0",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "susy": "^3.0.5",
    "ts-node": "^9.0.0",
    "typescript": "^3.7.2"
  }
}
